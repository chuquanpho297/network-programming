<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="sMeetingBody" prefHeight="726.0" prefWidth="1032.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="body_title" alignment="CENTER_LEFT" prefHeight="59.0" prefWidth="1032.0"
               text="Scheduled meeting list"/>
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <AnchorPane prefHeight="36.0" prefWidth="1042.0">
                    <children>
                        <DatePicker fx:id="start_date" layoutX="210.0" layoutY="9.0"/>
                        <DatePicker fx:id="end_date" layoutX="582.0" layoutY="8.0"/>
                        <Button fx:id="searchBtn" layoutX="913.0" layoutY="8.0" text="Search"/>
                        <Label layoutX="533.0" layoutY="11.0" prefHeight="17.0" prefWidth="38.0" text="End:"/>
                        <Label layoutX="171.0" layoutY="13.0" prefHeight="17.0" prefWidth="38.0" text="Start:"/>
                    </children>
                </AnchorPane>
            </children>
        </HBox>
        <AnchorPane prefHeight="200.0" prefWidth="200.0">
            <children>
                <TableView fx:id="tableView" layoutY="9.0" prefHeight="619.0" prefWidth="1032.0">
                    <columns>
                        <TableColumn fx:id="teacherNameColumn" prefWidth="290.0" text="Teacher name"/>
                        <TableColumn fx:id="meetingTypeColumn" prefWidth="160.0" text="Meeting Type"/>
                        <TableColumn fx:id="remainingParticipantsColumn" prefWidth="131.0"
                                     text="Remaning participants"/>
                        <TableColumn fx:id="startTimeColumn" prefWidth="120" text="Start Time"/>
                        <TableColumn fx:id="endTimeColumn" prefWidth="120.0" text="End Time"/>
                        <TableColumn fx:id="actionColumn" prefWidth="150.0" text="Action"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </children>
        </AnchorPane>
        <Pagination fx:id="pagination" prefHeight="69.0" prefWidth="1032.0"/>
    </children>
</VBox>
