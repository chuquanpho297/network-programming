<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<VBox prefHeight="448.0" prefWidth="350.0" spacing="10" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="14.0" layoutY="14.0" text="Content:"/>
        <TextField fx:id="contentField" layoutX="14.0" layoutY="30.0" prefHeight="26.0" prefWidth="322.0"/>
    </AnchorPane>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="14.0" layoutY="14.0" text="Meeting Type:"/>
        <ComboBox fx:id="meetingTypeField" layoutX="14.0" layoutY="31.0" prefHeight="26.0" prefWidth="322.0">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Select Options"/>
                    <String fx:value="One To One"/>
                    <String fx:value="Group"/>
                </FXCollections>
            </items>
        </ComboBox>
    </AnchorPane>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="14.0" layoutY="14.0" text="Participant Number:"/>
        <TextField fx:id="participantNumberField" layoutX="15.0" layoutY="31.0" prefHeight="26.0" prefWidth="322.0"/>
    </AnchorPane>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="14.0" layoutY="14.0" text="Date:"/>
        <DatePicker fx:id="dateField" layoutX="15.0" layoutY="31.0" prefHeight="26.0" prefWidth="322.0"/>
    </AnchorPane>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="14.0" layoutY="14.0" text="Start Time:"/>
        <ComboBox fx:id="startTimeField" layoutX="15.0" layoutY="31.0" prefHeight="26.0" prefWidth="322.0"/>
    </AnchorPane>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Label layoutX="14.0" layoutY="14.0" text="End Time:"/>
        <ComboBox fx:id="endTimeField" layoutX="15.0" layoutY="31.0" prefHeight="26.0" prefWidth="322.0"/>
    </AnchorPane>
    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <Button fx:id="saveButton" layoutX="136.0" layoutY="11.0" prefHeight="26.0" prefWidth="78.0" text="Save"/>
    </AnchorPane>
</VBox>
